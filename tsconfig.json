{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./",
    "rootDirs": [
      "./",
      "./commands",
      "./events"
    ],
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": true,
  },
  "exclude": [
    "./node_modules"
  ],
  "ts-node": {
    "files": true
  },
}